{
    "version": 2,
    "builds": [
      {
        "src": "server.mjs",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.mjs"
      },
      {
        "src": "/page.mjs",
        "dest": "/page.mjs"
      }
    ],
    

      "rewrites": [{ "source": "/", "destination": "https://google.com/" }]
    
    
}